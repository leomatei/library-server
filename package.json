{
  "name": "library-server",
  "version": "1.0.0",
  "description": "node server for my library project",
  "main": "index.ts",
  "scripts": {
    "build": "rimraf dist && npx tsc -p .",
    "prestart": "npm run build",
    "start": "node --env-file=.env dist/index.js",
    "predev": "npm run build",
    "dev": "concurrently \"npx tsc -w\" \"nodemon --env-file=.env dist/index.js\""
  },
  "type": "module",
  "author": "leo matei",
  "license": "ISC",
  "keywords": [],
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@vercel/postgres": "^0.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-graphql": "0.12.0",
    "graphql": "15.3.0",
    "graphql-http": "^1.22.0",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
